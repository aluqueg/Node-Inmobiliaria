<!DOCTYPE html>
<html>
  <head>
    <title>home museo</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <link rel="stylesheet" href="/stylesheets/formedithouse.css">
    
  </head>
  <body>
    <header><%- include('modules/navbar') %></header>

    <section>
      <img class="img" src="/images/OIG1.jpeg" alt="logo">
      <h1 class="text-center m-5">Formulario edici칩n de casa</h1>
    </section>

    <main>
      <section>
        <div class="fondo d-flex flex-column align-items-center text-center">
      
        <form class="m-5" action="/houses/editHouse/<%= edit.h_id %>/<%= edit.r_id %>" method="post" enctype="multipart/form-data">
          <div class="mb-3">
            <label for="location" class="form-label">Localidad</label>
            <input
              type="text"
              class="form-control"
              id="location"
              aria-describedby="titleHelp"
              name="location"
              value="<%= edit.location %>"
            />
          </div>
          <div class="mb-3">
            <label for="address" class="form-label">Direcci칩n</label>
            <input
              type="text"
              class="form-control"
              id="address"
              aria-describedby="estyleHelp"
              name="address"
              value="<%= edit.address %>"
            />
          </div>
            <div class="mb-3">
              <label for="year" class="form-label">A침o</label>
              <input
                type="text"
                class="form-control"
                id="year"
                aria-describedby="estyleHelp"
                name="year"
                value="<%= edit.year %>"
              />
            </div>
            <div class="mb-3">
              <label for="mtrs" class="form-label">Metros</label>
              <input
                type="text"
                class="form-control"
                id="mtrs"
                aria-describedby="estyleHelp"
                name="mtrs"
                value="<%= edit.mtrs %>"
              />
            </div>
              <div class="mb-3">
                <label for="h_drescription" class="form-label">Descripci칩n</label>
                <textarea
                type="text"
                class="form-control"
                id="h_description"
                aria-describedby="estyleHelp"
                name="h_description"><%= edit.h_description %>"</textarea>
          </div>
      
      
          <button type="submit" class="btn btn-success">Submit</button>
        </form>
        </div>
      </section>
    </main>
  

<%- include('modules/footer') %>


    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
  </body>
</html>