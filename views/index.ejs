<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Inmobiliaria</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">

    <link rel="stylesheet" href="/stylesheets/index.css">
  </head>
  <body>
    <header>
      <%- include('modules/navbar') %>
    </header>
    <div class="background">
      <img src="/images/OIG1.jpeg" alt="">
    </div>
    <h1 class="text-center m-5">Bienvenido al portal inmobiliario</h1>


<main>
  <section>
    <div class="container-fluid">
      <div class="row justify-content-center ">
  
  
        <div class="col d-flex flex-wrap justify-content-center gap-4 text-center">
          <% result.forEach((elem)=>{ %>
                <div class="card" style="width: 35rem;">
                    <img src="/images/inmobiliaria/<%= elem.r_img %>" class="card-img-top" alt="...">
                    <div class="card-body">
                      <h2 class="card-title"><%= elem.name %></h2>
                      <h5 class="card-text"><%= elem.email %> <%= elem.tel %></h5>
                      <a  href="/oneInm/<%= elem.r_id %>" class="btn btn-success m-5">MÃ¡s Info</a>
  
                    </div>
                  </div>
                <% }) %>
          </div>
  
  
  
      </div>
    </div>
  </section>
</main>


<%- include('modules/footer') %>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
  </body>
</html>